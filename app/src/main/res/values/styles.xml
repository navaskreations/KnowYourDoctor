<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="DTitle">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/colorgray1gray</item>
        <item name="android:textSize">20dp</item>
        <item name="android:background">@color/colorWhite</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:gravity">fill_vertical</item>

    </style>


    <style name="DOther">
        <item name="android:textSize">18dp</item>
        <item name="android:background">@color/colorWhite</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:textColor">@color/colorgray0gray</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:typeface">normal</item>
        <item name="android:textStyle">bold|italic</item>
        <item name="android:gravity">fill_vertical</item>

    </style>

    <style name="DImg">
        <item name="android:background">@color/colorDarkGray</item>
        <item name="android:layout_width">88dp</item>
        <item name="android:layout_height">78dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_marginLeft">10dp</item>
    </style>


</resources>
